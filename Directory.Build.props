<Project>
  <PropertyGroup>
    <Version>1.0.0-pre2</Version>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>
  
  <PropertyGroup Label="Shared NuGet Package Information">
	<PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
    <Authors>David Rettenbacher</Authors>
    <Company />
    <Copyright>Copyright Â© David Rettenbacher 2021</Copyright>
    <PackageProjectUrl>https://github.com/warappa/OpenTransSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/warappa/OpenTransSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>bmecat bmecat-2005 opentrans opentrans-21</PackageTags>
    <PackageReleaseNotes>
		1.0.0-pre2
		- #2: Invalid XML Mapping for ClassificationGroupFeatureTemplate.Values
		- #3: DOCTYPE Causes Exception
		- #4: Drop support for now unsupported .NET Core 2.1
		- #5: Add proper support for .NET 6
	</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<IncludeSymbols>true</IncludeSymbols>
	<SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
  </PropertyGroup>

  <ItemGroup Label="SourceLink Support">
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All"/>
  </ItemGroup>

</Project>